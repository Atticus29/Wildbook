form.form.ng-cloak(novalidate, wb-key-handler-form)
    md-content
        md-tabs(md-border-bottom md-dynamic-height md-selected="selectedTabIndex")
            md-tab(label="Data")
                div(layout="row" flex="100")
                    wb-encounter-search-fields
                div(layout="row" flex="100")
                    wb-individual-search-fields
                md-button.md-whiteframe-1dp(type="submit" ng-click="search()" aria-label="Search") Search
            md-tab(label="Results")
                .ag-fresh(ag-grid="gridOptions", style="height: 400px;")
