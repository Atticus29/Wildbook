        ],
        "tablesorter": [
            "bower_components/tablesorter/jquery.metadata.js",
            "bower_components/tablesorter/jquery.tablesorter.js",
            "bower_components/tablesorter/themes/**"
        ],
        "ag-grid": [
            "bower_components/ag-grid/dist/ag-grid.min.js",
            "bower_components/ag-grid/dist/ag-grid.min.css",
            "bower_components/ag-grid/dist/theme-fresh.min.css"
        ]
